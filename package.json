{
  "main" : "dist/plugin.cjs.js",
  "name" : "@webnativellc/capacitor-navigation-bar",
  "module" : "dist/esm/index.js",
  "peerDependencies" : {
    "@capacitor/core" : ">=7.0.0"
  },
  "description" : "Capacitor Plugin for Navigation Bar",
  "types" : "dist/esm/index.d.ts",
  "prettier" : "@ionic/prettier-config",
  "version" : "7.1.0",
  "keywords" : [
    "capacitor",
    "plugin",
    "native",
    "navigation",
    "bar"
  ],
  "eslintConfig" : {
    "extends" : "@ionic/eslint-config/recommended"
  },
  "swiftlint" : "@ionic/swiftlint-config",
  "license" : "MIT",
  "private" : false,
  "capacitor" : {
    "ios" : {
      "src" : "ios"
    },
    "android" : {
      "src" : "android"
    }
  },
  "files" : [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "WebnativellcCapacitorNavigationBar.podspec",
    "ios/Sources",
    "ios/Tests",
    "Package.swift"
  ],
  "unpkg" : "dist/plugin.js",
  "author" : "hugotomazi",
  "devDependencies" : {
    "@capacitor/android" : "^7.0.0",
    "eslint" : "^8.57.0",
    "swiftlint" : "^2.0.0",
    "@ionic/swiftlint-config" : "^2.0.0",
    "typescript" : "~4.7.4",
    "@capacitor/core" : "^7.0.0",
    "prettier-plugin-java" : "^2.6.6",
    "@capacitor/docgen" : "^0.3.0",
    "prettier" : "^3.4.2",
    "@ionic/prettier-config" : "^4.0.0",
    "rollup" : "^4.30.1",
    "@ionic/eslint-config" : "^0.4.0",
    "rimraf" : "^6.0.1",
    "@capacitor/ios" : "^7.0.0"
  },
  "bugs" : {
    "url" : "https://github.com/hugotomazi/navigation-bar/issues"
  },
  "scripts" : {
    "clean" : "rimraf ./dist",
    "docgen" : "docgen --api NavigationBarPlugin --output-readme README.md --output-json dist/docs.json",
    "prepublishOnly" : "npm run build",
    "eslint" : "eslint . --ext ts",
    "watch" : "tsc --watch",
    "fmt" : "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "verify:android" : "cd android && ./gradlew clean build test && cd ..",
    "verify:web" : "npm run build",
    "verify:ios" : "xcodebuild -scheme WebnativellcCapacitorNavigationBar -destination generic/platform=iOS",
    "lint" : "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier" : "prettier \"**/*.{css,html,ts,js,java}\" --plugin=prettier-plugin-java",
    "verify" : "npm run verify:ios && npm run verify:android && npm run verify:web",
    "build" : "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "swiftlint" : "node-swiftlint"
  },
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/hugotomazi/navigation-bar.git"
  }
}